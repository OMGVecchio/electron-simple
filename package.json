{
  "name": "electron-simple",
  "version": "1.0.0",
  "description": "simple application based on electron with react",
  "main": "/app/main/index.js",
  "scripts": {
    "server": "cross-env NODE_ENV=dev webpack-dev-server",
    "electron": "cross-env NODE_ENV=production electron app/main/index.js",
    "dev": "node ./task/comb.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:mac": "cross-env NODE_ENV=production webpack && electron-packager ./ sword --platform=darwin --arch=x64  --overwrite --out ./release/$npm_package_version --version=0.37.8 --ignore='(.github|.git|.DS_Store|TODOS.md|imgPreview|dist)'",
    "build:win32": "cross-env NODE_ENV=production webpack && electron-packager ./ sword --platform=win32 --arch=ia32  --overwrite --out ./release/$npm_package_version --version=0.37.8 --ignore='(.github|.git|.DS_Store|TODOS.md|imgPreview|dist)'",
    "build:win64": "cross-env NODE_ENV=production webpack && electron-packager ./ sword --platform=win32 --arch=x64  --overwrite --out ./release/$npm_package_version --version=0.37.8 --ignore='(.github|.git|.DS_Store|TODOS.md|imgPreview|dist)'"
  },
  "keywords": [
    "electron",
    "react*",
    "stylus",
    "typescript",
    "webpack2"
  ],
  "author": "vecchio",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-import": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.1",
    "electron": "^1.6.8",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "gulp": "^3.9.1",
    "html-webpack-plugin": "^2.28.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.3",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.17.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "ts-loader": "^2.0.3",
    "typescript": "^2.3.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5",
    "electron-packager": "^7.0.3",
    "electron-prebuilt": "^0.37.0"
  },
  "dependencies": {
    "antd": "^2.13.1",
    "axios": "^0.16.2",
    "lodash": "^4.17.4",
    "lowdb": "^1.0.0",
    "marked": "^0.3.6",
    "moment": "^2.18.1",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-markdown": "^2.5.0",
    "react-router": "3.0.0",
    "tracking": "^1.1.3"
  }
}
